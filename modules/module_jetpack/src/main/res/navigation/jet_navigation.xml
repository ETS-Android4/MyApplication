<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.william.my.module.fragment.PrimaryFragment"
        android:label="@string/jet_nav_home"
        tools:layout="@layout/basics_fragment_primary" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.william.my.module.fragment.PrimaryVarFragment"
        android:label="@string/jet_nav_dashboard"
        tools:layout="@layout/basics_fragment_primary_var" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.william.my.module.fragment.BasicRecyclerFragment"
        android:label="@string/jet_nav_notifications"
        tools:layout="@layout/basics_fragment_recycler" />

</navigation>